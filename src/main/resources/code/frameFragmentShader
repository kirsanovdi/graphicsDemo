#version 330 core

out vec4 FragColor;
in vec2 texCoords;

uniform sampler2D screenTexture;

const float offset_x = 1.0f / 1900.0f;
const float offset_y = 1.0f / 1000.0f;


void main()
{
    vec4 color = texture(screenTexture, texCoords);
    FragColor = color;
    //FragColor = vec4(0.0f, 0.0f, 0.6f, 1.0f);
}